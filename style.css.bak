/* The close button */
.closebtn {
  margin-left: 15px;
  color: white;
  font-weight: bold;
  float: right;
  font-size: 22px;
  line-height: 20px;
  cursor: pointer;
  transition: 0.3s;
}

/* When moving the mouse over the close button */
.closebtn:hover {
  color: black;
}

.popup {
  position: relative;
  display: inline-block;
  cursor: pointer;
}

/* The actual popup (appears on top) */
.popup .popuptext {
  display: none;
  width: 300px;
  margin: 0px 0px 0px 130px;
  background-color: #111;
  color: #fff;
  text-align: center;
  border-radius: 13px;
  padding: 11px 16px;
  position: fixed;
  top: 0px;
  z-index: 1;

}

/* Popup arrow */
.popup .popuptext::after {
  content: "";
  position: absolute;
  bottom: 100%;
  left: 50%;
  margin-left: -5px;
  border-width: 5px;
  border-style: solid;
  border-color: #555 transparent transparent transparent;
}

/* Toggle this class when clicking on the popup container (hide and show the popup) */
.popup .show {
  -webkit-animation: fadeIn 1s;
  animation: fadeIn 1s
}
.form-popup {
  display: none;
  position: fixed;
  border: collapse;
  right: 25%; 
  bottom: 1%;
  z-index: 1;
  width: 40%;
  background-color: gray;
  border-radius: 22px;
  border: 3px solid #f1f1f1;
  justify-content: center;
  text-align: center;
}
.form-popup textarea {
  width: 90%;
  padding: 15px;
  margin: 5px 0 22px 0;
  border: none;
  background: #f1f1f1;
  resize: none;
  min-height: 200px;
}
.form-popup .cancel {
  background-color: red;
}
.dropdowncontent {
  display: none;
  position: relative;
  margin: 0px 0px 0px 0;
  background-color: #f1f1f1;
  min-width: 30px;
  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
  z-index: 1;
  border-radius: 12px;
  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
}
.dropdowncontent a {
  color: black;
  padding: 6px 10px;
  text-decoration: none;
  display: block;
}
.dropdowncontent a:hover {background-color: #ddd;}
.active, .dropdowncontent a: {background-color: #ddd; color: red;}

